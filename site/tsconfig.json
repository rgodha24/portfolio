{
   "extends": "astro/tsconfigs/strict",
   "compilerOptions": {
      "jsx": "preserve",
      "jsxImportSource": "solid-js",
      "importsNotUsedAsValues": "error",
      "esModuleInterop": true,
      "strict": true,
      "strictNullChecks": true
   },
   "include": [".astro/types.d.ts", "**/*"],
   "exclude": ["dist"]
}
